{
  "name": "bubble-sort-service",
  "version": "0.0.1",
  "description": "You ever get tired of writing your own sorting function? Well so did I. So why rewrite what we do best. ",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build-docker": "docker build . -t bubble-sort-service",
    "clean-build-docker": "docker build . -t bubble-sort-service --no-cache",
    "docker-run": "docker run -p 8000:8000 -d bubble-sort-service",
    "docker-run-local": "docker run -e BUBBLE_SORT_ARRAY='[6, 2, [4, 3],[[[5], null], 1]]' bubble-sort-service"
  },
  "author": "jmaleonard",
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-plugin-jest": "^21.18.0",
    "husky": "^1.3.1",
    "jest": "^23.4.2",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
